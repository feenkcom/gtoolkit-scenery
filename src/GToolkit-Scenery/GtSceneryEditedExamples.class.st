{ #category : #scenery }
GtSceneryEditedExamples >> eyeconScenery [
	"NB: Generated by GtScenery>>#storeAsExampleMethod -- do not edit."

	<gtExample>
	<noTest>
	<storedScenery>
	<return: #GtScenery>
	| scenery item1 |
	scenery := GtScenery new.
	item1 := scenery
			addSceneryItem: (GtSceneryPictureStencil new
					fileReference: FileLocator gtResource / 'feenkcom' / 'gtoolkit-demos' / 'doc' / 'reflection'
							/ 'eyecon.png') asItem.
	item1 position: 0 @ 0.
	item1 extent: 184.8927882491589 @ 150.0.
	scenery storeStringMethodName: 'GtSceneryEditedExamples>>#eyeconScenery'.
	^ scenery
]

